package collection;

import java.io.*;
import java.util.*;

public class linklist {
	public static void main(String[] args) {
		List<String> l1=new LinkedList<String>();
		
		// adding elements
		l1.add("delhi");
		l1.add("dubai");
		l1.add("newyork");
		System.out.println(l1);
		
		//removing elements
		l1.remove(2);
		System.out.println(l1);
		l1.remove("delhi")
	}

}
