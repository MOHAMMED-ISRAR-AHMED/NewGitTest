package practice;

class  hi extends Thread {
	public void run() { //used run()
		for(int i=0;i<=5;i++) {
			System.out.println("hi");
			try { Thread.sleep(1000);}
			catch (Exception e) {
				// TODO: handle exception
			}}}}

class hello extends Thread {
	public void run() { //used run()
		for(int i=0;i<=5;i++) {
			System.out.println("hello");
			try { Thread.sleep(1000);}
			catch (Exception e) {
				// TODO: handle exception
			}
			}}}

class demo1{
	public static void main(String[] args) {
		hi h1=new hi();
		hello h2=new hello();
		
		h1.run();
		try { Thread.sleep(1000);}
		catch (Exception e) {
			// TODO: handle exception
		}
		h2.run();
	
	}
}